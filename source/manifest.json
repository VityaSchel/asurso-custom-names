{
  "name": "Кастомные названия предметов для АСУ РСО",
  "version": "1.0",
  "description": "Асу асу асу асу асу",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png",
      "512": "images/icon512.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "512": "images/icon512.png"
  },
  "content_scripts": [
    {
      "matches": ["https://asurso.ru/angular/school/studentdiary/"],
      "js": ["content-script.js"]
    }
  ],
  "manifest_version": 2
}
